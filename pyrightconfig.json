{
  "pythonVersion": "3.12",
  "venvPath": ".",
  "venv": ".venv",
  "typeCheckingMode": "all",
  "failOnWarnings": true,
  "reportUnreachable": "error",
  "include": [
    "packages/envoi/envoi"
  ],
  "extraPaths": [
    "packages/envoi",
    "packages/code",
    "packages/cli"
  ],
  "exclude": [
    ".venv",
    "**/.venv/**",
    "**/*.egg-info/**",
    "output",
    "**/node_modules/**"
  ],
  "executionEnvironments": [
    {
      "root": ".",
      "extraPaths": [
        "packages/envoi",
        "packages/code",
        "packages/cli"
      ]
    }
  ]
}
