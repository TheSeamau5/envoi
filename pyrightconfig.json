{
  "pythonVersion": "3.12",
  "venvPath": ".",
  "venv": ".venv",
  "typeCheckingMode": "basic",
  "include": [
    "packages",
    "examples"
  ],
  "extraPaths": [
    "packages/envoi",
    "packages/code",
    "packages/cli"
  ],
  "exclude": [
    ".venv",
    "**/.venv/**",
    "**/*.egg-info/**",
    "output",
    "**/node_modules/**"
  ],
  "executionEnvironments": [
    {
      "root": "packages/envoi"
    },
    {
      "root": "packages/code"
    },
    {
      "root": "packages/cli"
    },
    {
      "root": "examples/c_compiler/environment"
    },
    {
      "root": "examples/gameboy_emulator/environment"
    },
    {
      "root": "examples/c_compiler/task"
    },
    {
      "root": "examples/gameboy_emulator/task"
    }
  ]
}
